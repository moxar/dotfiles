# Import colors
. ~/.dotfiles/colors
. ~/.dotfiles/git
. ~/.dotfiles/git-completion
. ~/.dotfiles/go
. ~/.dotfiles/to

# PS
PS1=${BOLD}${YELLOW}"\t "${BOLD}${BLUE}"\u "${BOLD}${GREEN}"\w "${RED}"\$(parse_git_branch)\$(parse_git_status)"${BOLD}${BLUE}"\\$ "${DEFAULT}
alias ls='ls --color=auto'
alias ll="ls -h"

# alias bashrc
alias bashrc="vim ~/.bashrc && source ~/.bashrc"

# PATH
export PATH=$PATH:.
