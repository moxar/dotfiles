[alias]
	lo = log --graph --pretty=format:'%C(red)%h%C(reset) -%C(yellow)%d%C(reset) %s %C(green)(%an) %C(blue)<%cr>%C(reset)'
	st = status
	co = checkout
	up = pull
	ci = commit
	br = branch
	cp = cherry-pick
	puf = push -f
	prev = checkout HEAD~1
	fu = rebase -i HEAD~2
	hunt = log --diff-filter=A --
	amend = commit --amend
        count = rev-list @ --count
	ca = commit --amend -a --no-edit
[color]
	ui = true
[core]
	editor = vim
	pager = less
	excludesfile = ~/.gitignore
[pull]
	rebase = true
[fetch]
	all = true
 	prune = true
	tags = true
[merge]
	ff = false
[push]
	default = upstream
